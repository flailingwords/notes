        "swr": "^2.3.6",
        "vite-tsconfig-paths": "^5.1.4",
        "yaml": "^2.8.1",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@storybook/addon-themes": "^9.1.3",
        "@storybook/react-vite": "^9.1.3",
        "@tailwindcss/vite": "^4.1.12",
        "@tanstack/eslint-plugin-query": "^5.83.1",
        "@tanstack/router-devtools": "^1.131.27",
        "@tanstack/router-plugin": "^1.131.27",
        "@tyisi/config-changelog": "^2.0.0",
        "@tyisi/config-eslint": "^5.0.0",
        "@tyisi/config-prettier": "^2.0.0",
        "@tyisi/config-stylelint": "^2.0.0",
        "@types/react": "^19.1.11",
        "@types/react-dom": "^19.1.7",
        "@typescript-eslint/eslint-plugin": "^8.41.0",
        "@typescript-eslint/parser": "^8.41.0",
        "@vitejs/plugin-react": "^5.0.1",
        "eslint": "^9.34.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "eslint-plugin-storybook": "^9.1.3",
        "generate-react-cli": "^8.4.9",
        "git-cz": "^4.8.0",
        "husky": "^9.1.7",
        "just-install": "^2.0.2",
        "msw": "^2.10.5",
        "msw-storybook-addon": "^2.0.5",
        "prettier": "^3.6.2",
        "prettier-plugin-sh": "^0.18.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "storybook": "^9.1.3",
        "stylelint": "^16.23.1",
        "stylelint-config-clean-order": "^7.0.0",
        "stylelint-config-property-sort-order-smacss": "^10.0.0",
        "stylelint-config-standard": "^39.0.0",
        "stylelint-order": "^7.0.0",
        "stylelint-prettier": "^5.0.3",
        "tailwindcss": "^4.1.12",
        "typescript": "^5.2.2",
        "vite": "^7.1.3",
        "wireit": "^0.14.12"
    },
    "license": "DUAL",
    "licenses": [
        {
            "type": "MIT"
        },
        {
            "type": "GPL-3.0"
        }
    ],
    "name": "notes",
    "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
    "private": true,
    "scripts": {
        "build": "wireit",
        "build:gh-pages": "wireit",
        "build:storybook": "wireit",
        "commit": "git-cz",
        "dev": "wireit",
        "lint": "wireit",
        "prepare": "husky || true",
        "preview": "wireit",
        "storybook": "wireit",
        "type-check": "wireit"
    },
    "type": "module",
    "version": "0.0.0",
    "wireit": {
        "build": {
            "dependencies": [
                "build:schemas",
                "build:dist",
                "build:type-check"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:dist": {
            "clean": true,
            "command": "just build dist",
            "dependencies": [
                "build:schemas",
                "build:format",
                "build:lint",
                "build:docs",
                "build:type-check"
            ],
            "files": [
                "docs/**/*",
                "public/**/*",
                "src/**/*"
            ],
            "output": [
                "dist/"
            ]
        },
        "build:dist:gh-pages": {
            "clean": true,
            "command": "just build gh-pages",
            "dependencies": [
                "build:schemas",
                "build:format",
                "build:lint",
                "build:docs",
                "build:type-check"
            ],
            "files": [
                "docs/**/*",
                "public/**/*",
                "src/**/*"
            ],
            "output": [
                "dist/"
            ]
        },
        "build:docs": {
            "clean": true,
            "command": "just build docs",
            "files": [
                "docs/**/*"
            ],
            "output": [
                "public/docs/**/*"
            ]
        },
        "build:format": {
            "dependencies": [
                "build:schemas",
                "format"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:gh-pages": {
            "dependencies": [
                "build:dist:gh-pages",
                "build:type-check"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:lint": {
            "dependencies": [
                "build:schemas",
                "lint"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:schemas": {
            "dependencies": [
                "build:schemas:types",
                "build:schemas:guards"
            ],
            "files": [
                "src/schemas/*.ts"
            ]
        },
        "build:schemas:guards": {
            "dependencies": [
                "build:schemas:types"
            ],
            "command": "just build schemas-guards",
            "files": [
                "src/schemas/*.ts",
                "!src/schemas/guards.ts"
            ],
            "output": [
                "src/schemas/guards.ts"
            ]
        },
        "build:schemas:types": {
            "command": "just build schemas-types",
            "files": [
                "src/schemas/*.ts",
                "!src/schemas/guards.ts",
                "!src/schemas/types.ts"
            ],
            "output": [
                "src/schemas/types.ts"
            ]
        },
        "build:storybook": {
            "command": "just build storybook"
        },
        "build:type-check": {
            "dependencies": [
                "build:schemas",
                "type-check"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "dev": {
            "command": "just dev",
            "service": true
        },
        "format": {
            "command": "just format"
        },
        "lint": {
            "command": "just lint"
        },
        "preview": {
            "command": "vite preview",
            "service": true
        },
        "serve": {
            "command": "just serve",
            "service": true
        },
        "start": {
            "command": "just start",
            "service": true
        },
        "storybook": {
            "command": "just storybook",
            "service": true
        },
        "test": {
            "command": "just test"
        },
        "type-check": {
            "command": "just type-check"
        }
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
