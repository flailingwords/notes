    "devDependencies": {
        "@storybook/addon-themes": "^10.1.11",
        "@storybook/react-vite": "^10.1.11",
        "@tailwindcss/vite": "^4.1.12",
        "@tanstack/eslint-plugin-query": "^5.91.2",
        "@tanstack/router-devtools": "^1.0.0",
        "@tanstack/router-plugin": "^1.0.0",
        "@tyisi/config-changelog": "^2.0.0",
        "@tyisi/config-eslint": "^5.0.0",
        "@tyisi/config-prettier": "^2.0.0",
        "@tyisi/config-stylelint": "^2.0.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@vitejs/plugin-react": "^5.0.0",
        "eslint": "^9.0.0",
        "eslint-plugin-react-hooks": "^7.0.0",
        "eslint-plugin-react-refresh": "^0.4.0",
        "eslint-plugin-storybook": "^10.1.11",
        "generate-react-cli": "^9.0.0",
        "git-cz": "^4.0.0",
        "husky": "^9.1.7",
        "just-install": "^2.0.2",
        "msw": "^2.12.7",
        "msw-storybook-addon": "^2.0.6",
        "prettier": "^3.0.0",
        "prettier-plugin-sh": "^0.18.0",
        "prettier-plugin-tailwindcss": "^0.7.0",
        "storybook": "^10.1.11",
        "stylelint": "^16.26.1",
        "stylelint-config-clean-order": "^8.0.0",
        "stylelint-config-property-sort-order-smacss": "^10.0.0",
        "stylelint-config-standard": "^39.0.0",
        "stylelint-order": "^7.0.0",
        "stylelint-prettier": "^5.0.0",
        "tailwindcss": "^4.0.0",
        "typescript": "^5.9.3",
        "vite": "^7.0.0",
        "vite-tsconfig-paths": "^6.0.3",
        "wireit": "^0.14.12"
    },
    "license": "DUAL",
    "licenses": [
        {
            "type": "MIT"
        },
        {
            "type": "GPL-3.0"
        }
    ],
    "name": "notes",
    "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
    "private": true,
    "scripts": {
        "build": "wireit",
        "build:gh-pages": "wireit",
        "build:storybook": "wireit",
        "commit": "git-cz",
        "dev": "wireit",
        "lint": "wireit",
        "prepare": "husky || true",
        "preview": "wireit",
        "storybook": "wireit",
        "type-check": "wireit"
    },
    "type": "module",
    "version": "0.0.0",
    "wireit": {
        "build": {
            "dependencies": [
                "build:schemas",
                "build:dist",
                "build:type-check"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:dist": {
            "clean": true,
            "command": "just build dist",
            "dependencies": [
                "build:schemas",
                "build:format",
                "build:lint",
                "build:docs",
                "build:type-check"
            ],
            "files": [
                "docs/**/*",
                "public/**/*",
                "src/**/*"
            ],
            "output": [
                "dist/"
            ]
        },
        "build:dist:gh-pages": {
            "clean": true,
            "command": "just build gh-pages",
            "dependencies": [
                "build:schemas",
                "build:format",
                "build:lint",
                "build:docs",
                "build:type-check"
            ],
            "files": [
                "docs/**/*",
                "public/**/*",
                "src/**/*"
            ],
            "output": [
                "dist/"
            ]
        },
        "build:docs": {
            "clean": true,
            "command": "just build docs",
            "files": [
                "docs/**/*"
            ],
            "output": [
                "public/docs/**/*"
            ]
        },
        "build:format": {
            "dependencies": [
                "build:schemas",
                "format"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:gh-pages": {
            "dependencies": [
                "build:dist:gh-pages",
                "build:type-check"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:lint": {
            "dependencies": [
                "build:schemas",
                "lint"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "build:schemas": {
            "dependencies": [
                "build:schemas:types",
                "build:schemas:guards"
            ],
            "files": [
                "src/schemas/*.ts"
            ]
        },
        "build:schemas:guards": {
            "dependencies": [
                "build:schemas:types"
            ],
            "command": "just build schemas-guards",
            "files": [
                "src/schemas/*.ts",
                "!src/schemas/guards.ts"
            ],
            "output": [
                "src/schemas/guards.ts"
            ]
        },
        "build:schemas:types": {
            "command": "just build schemas-types",
            "files": [
                "src/schemas/*.ts",
                "!src/schemas/guards.ts",
                "!src/schemas/types.ts"
            ],
            "output": [
                "src/schemas/types.ts"
            ]
        },
        "build:storybook": {
            "command": "just build storybook"
        },
        "build:type-check": {
            "dependencies": [
                "build:schemas",
                "type-check"
            ],
            "files": [
                "src/**/*"
            ]
        },
        "dev": {
            "command": "just dev",
            "service": true
        },
        "format": {
            "command": "just format"
        },
        "lint": {
            "command": "just lint"
        },
        "preview": {
            "command": "vite preview",
            "service": true
        },
        "serve": {
            "command": "just serve",
            "service": true
        },
        "start": {
            "command": "just start",
            "service": true
        },
        "storybook": {
            "command": "just storybook",
            "service": true
        },
        "test": {
            "command": "just test"
        },
        "type-check": {
            "command": "just type-check"
        }
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
